{
  "name": "tp-games",
  "version": "0.0.0",
  "author": "Justin Fernald",
  "homepage": "https://justinfernald.github.io/tp-games-lib",
  "scripts": {
    "dev": "vite --host --port 4040 -c vite.config.ts",
    "dev-c": "vite --host --port 4041 -c vite.controller.config.ts",
    "dev-h": "vite --host --port 4042 -c vite.hoster.config.ts",
    "lt": "npx localtunnel --port 4040",
    "lt-c": "npx localtunnel --port 4041",
    "lt-h": "npx localtunnel --port 4042",
    "dev-lt": "concurrently \"yarn dev-b\" \"yarn lt-b\"",
    "dev-c-lt": "concurrently \"yarn dev-c\" \"yarn lt-c\"",
    "dev-h-lt": "concurrently \"yarn dev-h\" \"yarn lt-h\"",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:format": "prettier . --list-different --write",
    "lint": "yarn lint:format && yarn lint:fix ",
    "lint:pre-commit": "eslint . --max-warnings=0",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@types/node": "^18.15.10",
    "firebase": "^9.19.1",
    "mobx": "^6.8.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/babel-plugin": "^11.10.6",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^7.0.7",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@types/w3c-generic-sensor": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.0.1",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "rollup-plugin-obfuscator": "^0.2.2",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "pre-commit": "lint:pre-commit",
  "license": "MIT"
}
