{
  "name": "@toilet-paper-games/core",
  "version": "0.0.0",
  "author": "Justin Fernald",
  "homepage": "https://Toilet-Paper-Games.github.io/core",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/tp-games-lib.es.js",
      "require": "./dist/tp-games-lib.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/tp-games-lib.cjs.js",
  "module": "dist/tp-games-lib.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run generate-barrels && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:format": "prettier . --list-different --write",
    "lint": "yarn lint:format && yarn lint:fix ",
    "lint:pre-commit": "eslint . --max-warnings=0",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-barrels": "barrelsby --delete -d ./src -e vite-env.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "firebase": "^9.22.0",
    "mobx": "^6.8.0",
    "mobx-react-lite": "^3.4.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@emotion/babel-plugin": "^11.10.6",
    "@storybook/addon-actions": "^7.0.15",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/builder-vite": "^7.0.18",
    "@storybook/react": "^7.3.2",
    "@storybook/testing-library": "^0.1.0",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.3",
    "@types/react-dom": "^18.0.11",
    "@types/w3c-generic-sensor": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "barrelsby": "^2.7.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "jsdom": "^22.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook-dark-mode": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0"
  },
  "pre-commit": "lint:pre-commit",
  "license": "UNLICENSED"
}
