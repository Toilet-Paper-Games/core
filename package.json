{
  "name": "@justinfernald/tp-games-lib",
  "version": "0.0.0",
  "author": "Justin Fernald",
  "homepage": "https://justinfernald.github.io/tp-games-lib",
  "type": "module",
  "files": [
    "dist"
  ],
  "directories": {
    "components": "./dist/library/components",
    "core": "./dist/library/core",
    "styles": "./dist/library/styles",
    "utils": "./dist/library/utils"
  },
  "exports": {
    ".": {
      "import": "./dist/tp-games.es.js",
      "require": "./dist/tp-games.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/tp-games.umd.js",
  "module": "./dist/tp-games.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "test": "echo \"Warn: no test specified\" && exit 0",
    "coverage": "echo \"Warn: no coverage specified\" && exit 0",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:format": "prettier . --list-different --write",
    "lint": "yarn lint:format && yarn lint:fix ",
    "lint:pre-commit": "eslint . --max-warnings=0",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "firebase": "^9.19.1",
    "mobx": "^6.8.0",
    "mobx-react-lite": "^3.4.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "@babel/core": "^7.21.4",
    "@emotion/babel-plugin": "^11.10.6",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/builder-vite": "^7.0.7",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.2.3",
    "@types/react-dom": "^18.0.11",
    "@types/w3c-generic-sensor": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^9.1.2",
    "barrelsby": "^2.7.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "rollup-plugin-obfuscator": "^1.0.3",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "pre-commit": "lint:pre-commit",
